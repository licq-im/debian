#: src/fifo.cpp:122
msgid ""
"\tadduser <uin>\n"
"\t\tAdd user with <uin> to your contact list.\n"
msgstr ""
"\tadduser <uin>\n"
"\t\tFügt den Benutzer mit der UIN <uin> zu Ihrer Kontaktliste hinzu.\n"

#: src/fifo.cpp:98
msgid ""
"\tauto_response <auto response>\n"
"\t\tSets the auto response message without\n"
"\t\tchanging the current status.\n"
msgstr ""
"\tauto_response <Auto-Antwort>\n"
"\t\tStellt die automatische Antwort ein, ohne den\n"
"\t\taktuellen Status dabei zu ändern.\n"

#: src/fifo.cpp:118
msgid ""
"\tdebuglvl <level>\n"
"\t\tSet what information is logged.\n"
"\t\tSee <level> in licq -h\n"
msgstr ""
"\tdebuglvl <level>\n"
"\t\tStellt ein, wieviel Informationen protokolliert werden.\n"
"\t\tSchauen Sie sich <level> in \"Licq -h\" an\n"

#: src/fifo.cpp:132
msgid ""
"\texit\n"
"\t\tCauses the Licq session to shutdown.\n"
msgstr ""
"\texit\n"
"\t\tBeendet die Licq-Sitzung.\n"

#: src/fifo.cpp:167
msgid ""
"\thelp <<command> | all>\n"
"\t\tPrint help information for <command> or for all commands.\n"
msgstr ""
"\thelp <<Kommando> | all>\n"
"\t\tZeigt die Hilfe zu <Kommando> oder für alle Kommandos an.\n"

#: src/fifo.cpp:147
msgid ""
"\tlist_plugins\n"
"\t\tLists the loaded UI plugins\n"
msgstr ""
"\tlist_plugins\n"
"\t\tZeigt die geladenen UI plugins an\n"

#: src/fifo.cpp:157
msgid ""
"\tlist_proto_plugins\n"
"\t\tLists the loaded protocol plugins\n"
msgstr ""
"\tlist_proto_plugins\n"
"\t\tZeigt die geladenen Protokoll-Plugins an\n"

#: src/fifo.cpp:150
msgid ""
"\tload_plugin <plugin>\n"
"\t\tLoads the UI plugin called <plugin>\n"
msgstr ""
"\tload_plugin <plugin>\n"
"\t\tLädt das UI plugin mit Namen <plugin>\n"

#: src/fifo.cpp:160
msgid ""
"\tload_proto_plugin <protoplugin>\n"
"\t\tLoads the protocol plugin called <protoplugin>\n"
msgstr ""
"\tload_proto_plugin <plugin>\n"
"\t\tLädt das Protokoll-Plugin mit Namen <plugin>\n"

#: src/fifo.cpp:102
msgid ""
"\tmessage <buddy> <message>\n"
"\t\tSend a message to the given buddy.\n"
msgstr ""
"\tmessage <Benutzer> <Nachricht>\n"
"\t\tSendet eine Nachricht an den Benutzer.\n"

#: src/fifo.cpp:114
msgid ""
"\tredirect <file>\n"
"\t\tRedirects stderr for\n"
"\t\tLicq to the given file.\n"
msgstr ""
"\tredirect <file>\n"
"\t\tLeitet die Fehlerausgabe von Licq in\n"
"\t\tdie angegebene Datei um.\n"

#: src/fifo.cpp:128
msgid ""
"\tsetpicture <filename> [<protocol>]\n"
"\t\tChanges picture for an account, or all accounts if no protocol entered.\n"
"\t\tAn empty filename will remove the current picture.\n"
msgstr ""
"\tsetpicture <Dateiname> [<Prokoll>]\n"
"\t\tÄndert das Bild für einen Zugang, oder alle Zugänge, falls kein "
"Protokoll angegeben wurde.\n"
"\t\tEin leerer Dateiname entfernt das aktuelle Bild.\n"

#: src/fifo.cpp:108
msgid ""
"\tsms <buddy> <message>\n"
"\tSend a SMS to the given buddy.\n"
msgstr ""
"\tsms <Benutzer> <Nachricht>\n"
"\tSendet eine SMS an den Benutzer.\n"

#: src/fifo.cpp:111
msgid ""
"\tsms <number> <message>\n"
"\tSend a SMS to the given cellular number.\n"
msgstr ""
"\tsms <Nummer> <Nachricht>\n"
"\tSendet eine SMS an die angegebene Handynummer.\n"

#: src/fifo.cpp:92
msgid ""
"\tstatus <[*]<status>> <auto response>\n"
"\t\tstatus: online, offline, na, away, occupied, dnd, ffc\n"
"\n"
"\t\tSets the status of the current Licq session\n"
"\t\tto that given (precede the status by a\n"
"\t\t\"*\" for invisible mode)\n"
msgstr ""
"\tstatus <[*]<Status>> <Auto-Antwort>\n"
"\t\tStatus: online, offline, na, away, occupied, dnd, ffc\n"
"\n"
"\t\tSetzt den Status in der aktuellen Licq-Sitzung\n"
"\t\t(Stellen Sie dem Status ein \"*\" voran um\n"
"\t\tden Unsichtbar-Modus einzuschalten)\n"

#: src/fifo.cpp:144
msgid ""
"\tui_hideuserlist\n"
"\t\tHide the contact list window\n"
msgstr ""

#: src/fifo.cpp:138
msgid ""
"\tui_message <buddy>\n"
"\t\tOpen the plugin message composer to <buddy>\n"
msgstr ""
"\tui_message <Benutzer>\n"
"\t\tÖffnet den \"Nachricht senden\"-Dialog des GUI-Plugins\n"

#: src/fifo.cpp:141
msgid ""
"\tui_showuserlist\n"
"\t\tShow and raise the contact list window\n"
msgstr ""

#: src/fifo.cpp:135
msgid ""
"\tui_viewevent [<buddy>]\n"
"\t\tShows the oldest pending event.\n"
msgstr ""
"\tui_viewevent [<Benutzer>]\n"
"\t\tZeigt dass älteste auf Abruf wartende Ereignis an.\n"

#: src/fifo.cpp:153
msgid ""
"\tunload_plugin <plugin>\n"
"\t\tUnloads the UI plugin called <plugin>.\n"
"\t\tUse list_plugins to see currently loaded UI plugins.\n"
msgstr ""
"\tunload_plugin <plugin>\n"
"\t\tEntlädt das UI-Plugin mit Namen <plugin>.\n"
"\t\tBenutze list_plugins um die aktuell geladenen UI-Plugins anzuzeigen.\n"

#: src/fifo.cpp:163
msgid ""
"\tunload_proto_plugin <protoplugin>\n"
"\t\tUnloads the protocol plugin called <protoplugin>.\n"
"\t\tUse list_proto_plugins to see currently loaded protocol plugins.\n"
msgstr ""
"\tunload_proto_plugin <plugin>\n"
"\t\tEntlädt das Protokoll-Plugin mit Namen <plugin>.\n"
"\t\tBenutze list_proto_plugins um die aktuell geladenen Protokoll-Plugins "
"anzuzeigen.\n"

#: src/fifo.cpp:105
msgid ""
"\turl <buddy> <url> [<description>]\n"
"\t\tSend a url to the given buddy.\n"
msgstr ""
"\turl <Benutzer> <URL> [<Beschreibung>]\n"
"\t\tSendet eine URL an den Benutzer.\n"

#: src/fifo.cpp:125
msgid ""
"\tuserinfo <buddy>\n"
"\t\tUpdates a buddy's user information.\n"
msgstr ""
"\tuserinfo <Benutzer>\n"
"\t\tAktualisiert die Benutzerinformationen von <Benutzer>.\n"

